{
  "name": "Study Buddy",
  "description": "Remove distractions and stay focused",
  "icons": {"128": "prog.png"},
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remove distractions and stay focused"
  },
  "manifest_version": 2
}
